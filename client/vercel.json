{
    "version": 2,
    "name": "comprarla-bot",
    "builds": [
      {
        "src": "client/next.config.mjs",
        "use": "@vercel/next"
      },
      {
        "src": "server/src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/$1"
      }
    ]
  }
  